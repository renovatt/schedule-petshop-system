generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Clients {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  name         String
  age          String
  sex          String
  street       String
  house_number String
  neighborhood String
  city         String
  contact      String
  created_at   DateTime @default(now())
}

model Schedules {
  id                 String   @id @default(auto()) @map("_id") @db.ObjectId
  tutor              String
  pet                String
  breed              String
  reference_image_id String
  age                String
  weight             String
  sex                String
  client             Boolean
  date               DateTime
  time               String
  created_at         DateTime @default(now())
}
